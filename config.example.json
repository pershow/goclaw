{
  "workspace": {
    "path": ""
  },
  "agents": {
    "defaults": {
      "model": "YOUR_DEFAULT_MODEL_HERE",
      "max_iterations": 15,
      "temperature": 1,
      "max_tokens": 8192,
      "context_tokens": 0,
      "limit_history_turns": 0
    }
  },
  "session": {
    "scope": "per-sender",
    "store": "",
    "reset": {
      "mode": "daily",
      "at_hour": 4,
      "idle_minutes": 60
    }
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "",
      "allowed_ids": []
    },
    "whatsapp": {
      "enabled": false,
      "bridge_url": "",
      "allowed_ids": []
    },
    "feishu": {
      "enabled": false,
      "app_id": "",
      "app_secret": "",
      "encrypt_key": "",
      "verification_token": "",
      "event_mode": "long_connection",
      "webhook_port": 8765
    },
    "qq": {
      "enabled": false,
      "app_id": "",
      "app_secret": "",
      "allowed_ids": []
    },
    "wework": {
      "enabled": false,
      "corp_id": "",
      "agent_id": "",
      "secret": "",
      "token": "",
      "encoding_aes_key": ""
    },
    "dingtalk": {
      "enabled": false,
      "client_id": "",
      "secret": "",
      "allowed_ids": []
    },
    "infoflow": {
      "enabled": false,
      "webhook_url": "",
      "token": "",
      "aes_key": "",
      "webhook_port": 18766,
      "allowed_ids": [],
      "accounts": {
        "bot1": {
          "enabled": false,
          "name": "Infoflow Bot 1",
          "webhook_url": "",
          "token": "",
          "aes_key": "",
          "webhook_port": 18766,
          "allowed_ids": []
        }
      }
    }
  },
  "providers": {
    "profiles": [
      {
        "name": "openai-primary",
        "provider": "openai",
        "api_key": "YOUR_OPENAI_API_KEY",
        "base_url": "https://api.openai.com/v1",
        "priority": 1,
        "streaming": true
      },
      {
        "name": "deepseek",
        "provider": "openai",
        "api_key": "YOUR_DEEPSEEK_API_KEY",
        "base_url": "https://api.deepseek.com",
        "priority": 2,
        "streaming": true
      },
      {
        "name": "kimi",
        "provider": "moonshot",
        "api_key": "YOUR_MOONSHOT_KIMI_API_KEY",
        "base_url": "https://api.moonshot.cn/v1",
        "priority": 2,
        "streaming": true
      },
      {
        "name": "ollama-local",
        "provider": "openai",
        "api_key": "ollama",
        "base_url": "http://localhost:11434/v1",
        "priority": 3,
        "streaming": false
      }
    ],
    "failover": {
      "enabled": false,
      "strategy": "round_robin",
      "default_cooldown": "5m"
    },
    "openrouter": {
      "api_key": "",
      "base_url": "",
      "timeout": 600,
      "max_retries": 3,
      "streaming": true,
      "extra_body": {
        "reasoning": {
          "enabled": false
        }
      }
    },
    "openai": {
      "api_key": "YOUR_OPENAI_API_KEY_HERE",
      "base_url": "https://api.deepseek.com",
      "timeout": 600,
      "extra_body": {}
    },
    "anthropic": {
      "api_key": "",
      "base_url": "",
      "timeout": 600
    },
    "moonshot": {
      "api_key": "YOUR_MOONSHOT_KIMI_API_KEY",
      "base_url": "https://api.moonshot.cn/v1",
      "timeout": 600,
      "streaming": true,
      "extra_body": {
        "thinking": { "type": "disabled" }
      }
    }
  },
  "gateway": {
    "host": "localhost",
    "port": 28789,
    "read_timeout": 30,
    "write_timeout": 30,
    "websocket": {
      "host": "0.0.0.0",
      "port": 28789,
      "path": "/ws",
      "enable_auth": false,
      "auth_token": "",
      "ping_interval": "30s",
      "pong_timeout": "60s",
      "read_timeout": "60s",
      "write_timeout": "10s"
    }
  },
  "tools": {
    "filesystem": {
      "allowed_paths": [],
      "denied_paths": []
    },
    "shell": {
      "enabled": true,
      "allowed_cmds": [],
      "denied_cmds": [
        "rm -rf",
        "dd",
        "mkfs"
      ],
      "timeout": 30,
      "working_dir": ""
    },
    "web": {
      "search_api_key": "",
      "search_engine": "travily",
      "timeout": 10
    },
    "browser": {
      "enabled": true,
      "headless": true,
      "timeout": 30
    }
  },
  "memory": {
    "backend": "builtin",
    "builtin": {
      "enabled": true,
      "database_path": "",
      "auto_index": true,
      "sync": {
        "watch": true,
        "watch_debounce_ms": 1500
      }
    },
    "qmd": {
      "command": "qmd",
      "enabled": false,
      "include_default": true,
      "paths": [
        {
          "name": "notes",
          "path": "~/notes",
          "pattern": "**/*.md"
        },
        {
          "name": "docs",
          "path": "~/Documents",
          "pattern": "**/*.md"
        }
      ],
      "sessions": {
        "enabled": false,
        "export_dir": "~/.goclaw/sessions/export",
        "retention_days": 30
      },
      "update": {
        "interval": "5m",
        "on_boot": true,
        "embed_interval": "60m",
        "command_timeout": "30s",
        "update_timeout": "120s"
      },
      "limits": {
        "max_results": 6,
        "max_snippet_chars": 700,
        "timeout_ms": 4000
      }
    }
  }
}